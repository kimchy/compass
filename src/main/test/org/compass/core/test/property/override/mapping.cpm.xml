<!DOCTYPE compass-core-mapping PUBLIC 
    "-//Compass/Compass Core Mapping DTD 2.2//EN"
    "http://www.compass-project.org/dtd/compass-core-mapping-2.2.dtd">

<compass-core-mapping package="org.compass.core.test.property.override">

    <class name="PropertyOverride" alias="po">
    
        <id name="id">
          <meta-data store="yes" index="un_tokenized">wrongId</meta-data>
        </id>
        
        <id name="id">
          <meta-data store="yes" index="un_tokenized">overrideId</meta-data>
        </id>
        
        <property name="value">
            <meta-data>wrongValue</meta-data>
        </property>

        <property name="value">
            <meta-data>overrideValue</meta-data>
        </property>

    </class>

    <class name="PropertyOverride" alias="po1">
    
        <id name="id">
          <meta-data store="yes" index="un_tokenized">wrongId</meta-data>
        </id>
        
        <id name="id">
          <meta-data store="yes" index="un_tokenized">overrideId</meta-data>
        </id>
        
        <property name="value" override="false">
            <meta-data>wrongValue</meta-data>
        </property>

        <property name="value" override="false">
            <meta-data>overrideValue</meta-data>
        </property>

    </class>

</compass-core-mapping>
